{
  "name": "typedb-studio",
  "version": "2.1.3",
  "productName": "TypeDB Studio",
  "description": "IDE to perform knowledge engineering at scale and control everything in your TypeDB database, from development to production.",
  "keywords": [
    "typedb",
    "typeql",
    "typedb visualizer",
    "data visualizer",
    "graph visualizer"
  ],
  "scripts": {
    "build": "concurrently \"yarn build:main\" \"yarn build:renderer\"",
    "build:main": "cross-env NODE_ENV=production webpack --config ./build/configs/webpack.main.prod.babel.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./build/configs/webpack.renderer.prod.babel.js",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src",
    "package": "rm -rf src/dist && yarn build && electron-builder build --publish never",
    "postinstall": "node -r @babel/register build/scripts/check-native-dep.js && electron-builder install-app-deps && yarn cross-env NODE_ENV=development webpack --config ./build/configs/webpack.renderer.dev.dll.babel.js && opencollective-postinstall && yarn-deduplicate yarn.lock",
    "start": "node -r @babel/register ./build/scripts/check-port-in-use.js && yarn start:renderer",
    "start:main": "cross-env NODE_ENV=development electron -r ./build/scripts/babel-register ./src/main.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development webpack serve --config ./build/configs/webpack.renderer.dev.babel.js"
  },
  "build": {
    "appId": "com.vaticle.studio",
    "productName": "TypeDB Studio",
    "artifactName": "typedb-studio-${os}-${version}.${ext}",
    "files": [
      "dist/",
      "node_modules/",
      "index.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "afterSign": "build/scripts/notarize.js",
    "mac": {
      "target": [
        "dmg"
      ],
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "build/assets/entitlements.mac.plist",
      "entitlementsInherit": "build/assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "src",
      "buildResources": "build/assets",
      "output": "release"
    },
    "extraResources": [
      "./build/assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "vaticle"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaticle/typedb-workbase.git"
  },
  "author": {
    "name": "Vaticle",
    "url": "https://vaticle.com"
  },
  "contributors": [
    {
      "name": "Alex Walker",
      "email": "alex@vaticle.com"
    }
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/vaticle/typedb-workbase/issues"
  },
  "homepage": "https://github.com/vaticle/typedb-workbase#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "brace": "^0.11.1",
    "clsx": "1.1.1",
    "codemirror": "^5.62.0",
    "d3-force": "3.0.0",
    "electron-debug": "3.1.0",
    "electron-log": "4.3.0",
    "electron-updater": "4.3.5",
    "fontfaceobserver": "2.1.0",
    "history": "5.0.0",
    "moment": "^2.29.1",
    "pixi-viewport": "4.31.0",
    "pixi.js": "6.0.4",
    "prismjs": "1.24.1",
    "react": "17.0.1",
    "react-ace": "^9.4.1",
    "react-codemirror2": "^7.2.1",
    "react-collapse-pane": "2.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "regenerator-runtime": "0.13.7",
    "source-map-support": "0.5.19",
    "typedb-client": "https://repo.vaticle.com/repository/npm-snapshot/typedb-client/-/typedb-client-0.0.0-2fc88d5c19e277866c5034350041169bdc307094.tgz"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.12.9",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-react-constant-elements": "7.12.1",
    "@babel/plugin-transform-react-inline-elements": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@babel/register": "7.12.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@types/codemirror": "^5.60.2",
    "@types/d3-force": "3.0.2",
    "@types/history": "4.7.6",
    "@types/node": "14.14.10",
    "@types/prismjs": "1.16.6",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-router-dom": "5.1.6",
    "@types/webpack-env": "1.15.3",
    "babel-loader": "8.2.2",
    "babel-plugin-dev-expression": "0.2.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "chalk": "4.1.0",
    "concurrently": "5.3.0",
    "core-js": "3.7.0",
    "cross-env": "7.0.2",
    "css-loader": "5.0.1",
    "css-minimizer-webpack-plugin": "1.1.5",
    "detect-port": "1.3.0",
    "electron": "11.0.1",
    "electron-builder": "22.10.5",
    "electron-devtools-installer": "3.1.1",
    "electron-notarize": "1.0.0",
    "electron-rebuild": "2.3.2",
    "file-loader": "6.2.0",
    "identity-obj-proxy": "3.0.0",
    "mini-css-extract-plugin": "1.3.1",
    "node-sass": "5.0.0",
    "opencollective-postinstall": "2.0.3",
    "pixi.js-legacy": "6.0.4",
    "react-refresh": "0.9.0",
    "rimraf": "3.0.2",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "5.0.3",
    "typescript": "4.0.5",
    "url-loader": "4.1.1",
    "webpack": "5.5.1",
    "webpack-bundle-analyzer": "4.1.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.4.0",
    "yarn-deduplicate": "3.1.0"
  },
  "devEngines": {
    "node": ">=10.x",
    "npm": ">=6.x",
    "yarn": ">=1.21.3"
  },
  "browserslist": [],
  "renovate": {
    "extends": [
      "bliss"
    ],
    "baseBranches": [
      "next"
    ]
  }
}
